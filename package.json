    ],
    "production": [
      "> 1% in CN",
      "chrome >= 49"
    ]
  },
  "engines": {
    "node": ">=14.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
