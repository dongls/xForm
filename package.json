{
  "name": "xform",
  "private": true,
  "version": "0.4.4",
  "description": "Vue驱动的自定义表单套件。",
  "main": "dist/xform.core.js",
  "style": "dist/css/xform.core.css",
  "scripts": {
    "clean": "node scripts/clean",
    "build:code": "npm run build:core && npm run build:bootstrap",
    "build:core": "cross-env NODE_ENV=production RELEASE_PACKAGE=core webpack --config scripts/webpack/webpack.production.config.js",
    "build:bootstrap": "cross-env NODE_ENV=production RELEASE_PACKAGE=bootstrap webpack --config scripts/webpack/webpack.production.config.js",
    "build:example": "npm run clean && cross-env NODE_ENV=production webpack --config scripts/webpack/webpack.example.config.js",
    "build:docs": "npm run clean && cross-env TARGET=document NODE_ENV=production webpack --config scripts/webpack/webpack.document.config.js",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack/webpack.example.config.js",
    "dev:docs": "npm run clean && cross-env TARGET=document webpack-dev-server --config scripts/webpack/webpack.document.config.js",
    "lint": "eslint --ext .ts,.tsx,.vue .",
    "test": "jest --no-cache",
    "release": "node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dongls/xForm.git"
  },
  "homepage": "https://github.com/dongls/xForm",
  "keywords": [
    "vue",
    "form",
    "xform"
  ],
  "author": {
    "name": "dongls",
    "email": "173110115@qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@ant-design-vue/babel-plugin-jsx": "^1.0.0-alpha.6",
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@vue/compiler-sfc": "^3.0.0-beta.18",
    "@vue/test-utils": "^2.0.0-alpha.7",
    "autoprefixer": "^9.8.5",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "emoji-regex": "^9.0.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.4.0",
    "eslint-plugin-vue": "7.0.0-alpha.9",
    "execa": "^4.0.3",
    "file-loader": "^6.0.0",
    "highlight.js": "^10.1.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "less": "^3.12.0",
    "less-loader": "^6.2.0",
    "loader-utils": "^2.0.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "sass-resources-loader": "^2.0.3",
    "ts-jest": "^26.1.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "vue": "^3.0.0-rc.1",
    "vue-jest": "^5.0.0-alpha.0",
    "vue-loader": "^16.0.0-beta.4",
    "vue-router": "^4.0.0-beta.2",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version"
    ],
    "production": [
      "> 1% in CN",
      "chrome >= 49"
    ]
  },
  "engines": {
    "node": ">=14.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
